rofi.color-enabled: true
rofi.color-window: #2d2d2d, #2d2d2d, #cccccc
rofi.color-normal: #2d2d2d, #cccccc, #2d2d2d, #2d2d2d, #f99157
rofi.color-active: #2d2d2d, #cccccc, #2d2d2d, #2d2d2d, #99cc99
rofi.color-urgent: #2d2d2d, #cccccc, #2d2d2d, #2d2d2d, #f2777a

rofi.location: 5
rofi.width: 10
rofi.lines: 5
rofi.columns: 1
rofi.bw: 0
rofi.eh: 1
rofi.separator-style: solid
rofi.hide-scrollbar: true
rofi.font: Hack 12

XTerm.termName: xterm-256color

URxvt.vt100.saveLines: 4096
